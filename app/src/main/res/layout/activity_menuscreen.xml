<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C1B8B8">

    <ImageButton
        android:id="@+id/profile_image"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="130dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile_image_menu"
        tools:srcCompat="@drawable/profile_image_menu" />

    <ImageButton
        android:id="@+id/btn_Change_Profile"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_page"
        app:srcCompat="@drawable/btn_chage_profile" />

    <ImageButton
        android:id="@+id/btn_registered_account"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_my_bicycle"
        app:srcCompat="@drawable/btn_registered_account"
        tools:srcCompat="@drawable/btn_registered_account" />

    <ImageButton
        android:id="@+id/show_nickname"
        android:layout_width="130dp"
        android:layout_height="30dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="130dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_image"
        app:srcCompat="@drawable/show_nickname" />

    <ImageButton
        android:id="@+id/Usage_History"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="10dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_registered_account"
        app:srcCompat="@drawable/usage_history" />

    <ImageButton
        android:id="@+id/my_page"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/show_nickname"
        app:srcCompat="@drawable/btn_my_page" />

    <ImageButton
        android:id="@+id/btn_Setting"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="20dp"
        android:adjustViewBounds="false"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_History_of_use"
        app:srcCompat="@drawable/btn_setting" />

    <ImageButton
        android:id="@+id/btn_Share_History"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Usage_History"
        app:srcCompat="@drawable/btn_share_history" />

    <ImageButton
        android:id="@+id/btn_my_bicycle"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Change_Profile"
        app:srcCompat="@drawable/btn_my_bicycle" />

    <ImageButton
        android:id="@+id/btn_History_of_use"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Share_History"
        app:srcCompat="@drawable/btn_history_of_use" />

    <TextView
        android:id="@+id/text_show_nickname"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:text="닉네임"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/show_nickname"
        app:layout_constraintEnd_toEndOf="@+id/show_nickname"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/show_nickname"
        app:layout_constraintTop_toTopOf="@+id/show_nickname" />

    <TextView
        android:id="@+id/text_Usage_History"
        android:layout_width="101dp"
        android:layout_height="29dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="10dp"
        android:text="이용 내역"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Usage_History"
        app:layout_constraintEnd_toEndOf="@+id/Usage_History"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="@+id/Usage_History"
        app:layout_constraintTop_toTopOf="@+id/Usage_History" />

    <TextView
        android:id="@+id/text_my_bicycle"
        android:layout_width="60dp"
        android:layout_height="22dp"
        android:layout_marginStart="39dp"
        android:layout_marginEnd="1dp"
        android:gravity="center"
        android:text="내 자전거 등록"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_my_bicycle"
        app:layout_constraintEnd_toEndOf="@+id/btn_my_bicycle"
        app:layout_constraintStart_toStartOf="@+id/btn_my_bicycle"
        app:layout_constraintTop_toTopOf="@+id/btn_my_bicycle" />

    <TextView
        android:id="@+id/text_Change_Profile"
        android:layout_width="60dp"
        android:layout_height="22dp"
        android:layout_marginStart="50dp"
        android:gravity="center"
        android:text="프로필변경"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Change_Profile"
        app:layout_constraintEnd_toEndOf="@+id/btn_Change_Profile"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Change_Profile"
        app:layout_constraintTop_toTopOf="@+id/btn_Change_Profile" />

    <TextView
        android:id="@+id/text_Share_History"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="10dp"
        android:text="공유 내역"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Share_History"
        app:layout_constraintEnd_toEndOf="@+id/btn_Share_History"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Share_History"
        app:layout_constraintTop_toTopOf="@+id/btn_Share_History" />

    <TextView
        android:id="@+id/text_History_of_use"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="10dp"
        android:text="사용 내역"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_History_of_use"
        app:layout_constraintEnd_toEndOf="@+id/btn_History_of_use"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_History_of_use"
        app:layout_constraintTop_toTopOf="@+id/btn_History_of_use" />

    <TextView
        android:id="@+id/text_Registered_Account"
        android:layout_width="60dp"
        android:layout_height="22dp"
        android:layout_marginStart="24dp"
        android:gravity="center"
        android:text="등록 계좌"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_registered_account"
        app:layout_constraintEnd_toEndOf="@+id/btn_registered_account"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="@+id/btn_registered_account"
        app:layout_constraintTop_toTopOf="@+id/btn_registered_account" />

    <TextView
        android:id="@+id/text_my_page"
        android:layout_width="99dp"
        android:layout_height="36dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="40dp"
        android:text="MY PAGE"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/my_page"
        app:layout_constraintEnd_toEndOf="@+id/my_page"
        app:layout_constraintHorizontal_bias="0.564"
        app:layout_constraintStart_toStartOf="@+id/my_page"
        app:layout_constraintTop_toTopOf="@+id/my_page" />

    <TextView
        android:id="@+id/text_Setting"
        android:layout_width="62dp"
        android:layout_height="26dp"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="10dp"
        android:text="설정"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Setting"
        app:layout_constraintEnd_toEndOf="@+id/btn_Setting"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Setting"
        app:layout_constraintTop_toTopOf="@+id/btn_Setting" />

    <ImageButton
        android:id="@+id/btn_sign_out"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Setting"
        app:srcCompat="@drawable/btn_sign_out" />

    <TextView
        android:id="@+id/text_Sign_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="10dp"
        android:text="로그아웃"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_sign_out"
        app:layout_constraintEnd_toEndOf="@+id/btn_sign_out"
        app:layout_constraintStart_toStartOf="@+id/btn_sign_out"
        app:layout_constraintTop_toTopOf="@+id/btn_sign_out" />

</androidx.constraintlayout.widget.ConstraintLayout>