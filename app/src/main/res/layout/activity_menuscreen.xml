        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btn_show_nickname"
        app:layout_constraintEnd_toEndOf="@+id/btn_show_nickname"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_show_nickname"
        app:layout_constraintTop_toTopOf="@+id/btn_show_nickname" />

    <TextView
        android:id="@+id/text_Usage_History"
        android:layout_width="101dp"
        android:layout_height="29dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="10dp"
        android:text="이용 내역"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Usage_History"
        app:layout_constraintEnd_toEndOf="@+id/btn_Usage_History"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="@+id/btn_Usage_History"
        app:layout_constraintTop_toTopOf="@+id/btn_Usage_History" />

    <TextView
        android:id="@+id/text_my_bicycle"
        android:layout_width="60dp"
        android:layout_height="22dp"
        android:layout_marginStart="39dp"
        android:layout_marginLeft="39dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="10dp"
        android:text="내 자전거 등록"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_my_bicycle"
        app:layout_constraintEnd_toEndOf="@+id/btn_my_bicycle"
        app:layout_constraintStart_toStartOf="@+id/btn_my_bicycle"
        app:layout_constraintTop_toTopOf="@+id/btn_my_bicycle" />

    <TextView
        android:id="@+id/text_Change_Profile"
        android:layout_width="60dp"
        android:layout_height="22dp"
        android:layout_marginStart="50dp"
        android:gravity="center"
        android:text="프로필변경"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Change_Profile"
        app:layout_constraintEnd_toEndOf="@+id/btn_Change_Profile"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Change_Profile"
        app:layout_constraintTop_toTopOf="@+id/btn_Change_Profile" />

    <TextView
        android:id="@+id/text_Share_History"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="15dp"
        android:text="공유 내역"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Share_History"
        app:layout_constraintEnd_toEndOf="@+id/btn_Share_History"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Share_History"
        app:layout_constraintTop_toTopOf="@+id/btn_Share_History"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/text_History_of_use"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="15dp"
        android:text="사용 내역"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_History_of_use"
        app:layout_constraintEnd_toEndOf="@+id/btn_History_of_use"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_History_of_use"
        app:layout_constraintTop_toTopOf="@+id/btn_History_of_use"
        app:layout_constraintVertical_bias="0.666" />

    <TextView
        android:id="@+id/text_Sign_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="20dp"
        android:text="로그아웃"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Sign_out"
        app:layout_constraintEnd_toEndOf="@+id/btn_Sign_out"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Sign_out"
        app:layout_constraintTop_toTopOf="@+id/btn_Sign_out"
        app:layout_constraintVertical_bias="0.416" />

    <TextView
        android:id="@+id/text_Registered_Account"
        android:layout_width="60dp"
        android:layout_height="22dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:text="등록 계좌"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Registered_Account"
        app:layout_constraintEnd_toEndOf="@+id/btn_Registered_Account"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="@+id/btn_Registered_Account"
        app:layout_constraintTop_toTopOf="@+id/btn_Registered_Account"
        app:layout_constraintVertical_bias="0.75" />

    <TextView
        android:id="@+id/text_my_page"
        android:layout_width="99dp"
        android:layout_height="36dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="40dp"
        android:text="MY PAGE"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btn_my_page"
        app:layout_constraintEnd_toEndOf="@+id/btn_my_page"
        app:layout_constraintHorizontal_bias="0.564"
        app:layout_constraintStart_toStartOf="@+id/btn_my_page"
        app:layout_constraintTop_toTopOf="@+id/btn_my_page" />

    <TextView
        android:id="@+id/text_Setting"
        android:layout_width="62dp"
        android:layout_height="26dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:text="설정"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Setting"
        app:layout_constraintEnd_toEndOf="@+id/btn_Setting"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_Setting"
        app:layout_constraintTop_toTopOf="@+id/btn_Setting"
        app:layout_constraintVertical_bias="0.833" />

</androidx.constraintlayout.widget.ConstraintLayout>