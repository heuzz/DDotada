<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/precaution_before_starting1"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="376dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/precaution_before_starting1_background" />

    <ImageView
        android:id="@+id/precaution_before_starting1_information1"
        android:layout_width="357dp"
        android:layout_height="154dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="332dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/precaution_before_starting1"
        app:srcCompat="@drawable/precaution_before_starting1_information1" />

    <ImageView
        android:id="@+id/precaution_before_starting1_information2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/precaution_before_starting1"
        app:layout_constraintTop_toTopOf="@+id/precaution_before_starting1"
        app:srcCompat="@drawable/precaution_before_starting1_information2" />

    <ImageView
        android:id="@+id/precaution_before_starting1_view1"
        android:layout_width="60dp"
        android:layout_height="70dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting1_information2"
        app:srcCompat="@drawable/precaution_before_starting1_view1" />

    <ImageView
        android:id="@+id/precaution_before_starting1_view2"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting1_view3"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting1_information2"
        app:srcCompat="@drawable/precaution_before_starting1_view2" />

    <ImageView
        android:id="@+id/precaution_before_starting1_view3"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting1_view1"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting1_information2"
        app:srcCompat="@drawable/precaution_before_starting1_view3" />

    <ImageButton
        android:id="@+id/btn_precaution_before_starting1_next"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting1_view2"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting1_information2"
        app:srcCompat="@drawable/precaution_before_starting1_btn_view4" />

</androidx.constraintlayout.widget.ConstraintLayout>