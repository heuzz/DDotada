<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/precaution_before_starting3_background"
        android:layout_width="407dp"
        android:layout_height="365dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.519"
        app:srcCompat="@drawable/precaution_before_starting3_background" />

    <ImageView
        android:id="@+id/precaution_before_starting3_information1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toTopOf="@+id/precaution_before_starting3_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/precaution_before_starting3_information1" />

    <ImageView
        android:id="@+id/precaution_before_starting3_view1"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting3_background"
        app:srcCompat="@drawable/precaution_before_starting3_view1" />

    <ImageView
        android:id="@+id/precaution_before_starting3_view3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting3_view2"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting3_background"
        app:srcCompat="@drawable/precaution_before_starting3_view2" />

    <ImageView
        android:id="@+id/precaution_before_starting3_view2"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting3_view1"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting3_background"
        app:srcCompat="@drawable/precaution_before_starting3_view3" />

    <ImageButton
        android:id="@+id/precaution_before_starting3_btn_view4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:background="#FFFEFE"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting3_view3"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting3_background"
        app:srcCompat="@drawable/precaution_before_starting3_btn_view4" />

</androidx.constraintlayout.widget.ConstraintLayout>