<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/precaution_before_starting2_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/precaution_before_starting2_background" />

    <ImageView
        android:id="@+id/precaution_before_starting2_information1"
        android:layout_width="412dp"
        android:layout_height="166dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/precaution_before_starting2_information1" />

    <ImageView
        android:id="@+id/precaution_before_starting2_view1"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="439dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting2_information1"
        app:srcCompat="@drawable/precaution_before_starting2_view1" />

    <ImageView
        android:id="@+id/precaution_before_starting2_view2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="420dp"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting2_view1"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting2_information1"
        app:srcCompat="@drawable/precaution_before_starting2_view3" />

    <ImageView
        android:id="@+id/precaution_before_starting2_view3"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="440dp"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting2_view2"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting2_information1"
        app:srcCompat="@drawable/precaution_before_starting1_view2" />

    <ImageButton
        android:id="@+id/precaution_before_starting2_btn_view4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="448dp"
        android:backgroundTint="#FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/precaution_before_starting2_view3"
        app:layout_constraintTop_toBottomOf="@+id/precaution_before_starting2_information1"
        app:srcCompat="@drawable/precaution_before_starting2_btn_view4" />

</androidx.constraintlayout.widget.ConstraintLayout>